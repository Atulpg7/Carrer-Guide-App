                                            android:layout_height="30dp"
                                            android:background="@drawable/round_background_commerce_button"
                                            android:gravity="center"
                                            android:text="Coming Soon"
                                            android:paddingBottom="5dp"
                                            android:textColor="#fff"
                                            android:textSize="12sp"/>

